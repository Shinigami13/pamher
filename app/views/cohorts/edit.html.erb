<h1>Cohorts#edit</h1>
<p>Find me in app/views/cohorts/edit.html.erb</p>

  <%= form_for @cohort do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, value: @cohort.name %>

  <%= f.label :start_date %>
  <%= f.text_field :start_date, value: @cohort.start_date%>

  <%= f.label :end_date %>
  <%= f.text_field :end_date, value: @cohort.end_date %>

  <%= f.label :instructor_id %>
  <%= f.collection_select :instructor_id, Instructor.order(:lastname) %>

  <%= f.label :course_id %>
  <%= f.collection_select :course_id, Course.order(:name), :id, :name %>

  <%= f.submit%>


</div>
