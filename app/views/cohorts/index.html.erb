<h1>Cohorts#index</h1>
<p>Find me in app/views/cohorts/index.html.erb</p>
<%= link_to("Show", cohorts_index_path, :class => 'action edit') %>
<%= link_to("Edit", cohorts_edit_path, :class => 'action edit') %>

<%= form_for @cohort.each do |f| %>
  <%= f.text_field :name %>
  <%= f.date_field :start, required:true%>
  <%= f.date_field :end, required:true%>
  <%= f.submit %>
<% end %>
