<%= form_for @cohort, remote:true do |f| %>
  <%= f.text_field :name, required:true%>

  <%= f.collection_select :educator_id, Educator.order(:last_name), :id,:full_name%>

  <%= f.date_field :start, required:true%>
  <%= f.date_field :end, required:true%>

  <%= f.hidden_field :course_id, value: params[:course_id]%>

  <%= f.submit %>
<% end %>
